#(7,7,7) Triangle group
_RWS := rec
(
  isRWS := true,
  ordering := "wtlex",
  generatorOrder := [f,e,d,c,b,a],
  inverses := [f,e,d,c,b,a],
  weight := [7,7,7,1,1,1],
  equations :=
  [
    [(a*b)^7,IdWord],
    [(a*c)^7,IdWord],
    [(b*c)^7,IdWord],
    [f,a*b*a*b*a*b*a],
    [e,a*c*a*c*a*c*a],
    [d,b*c*b*c*b*c*b]
  ]
);

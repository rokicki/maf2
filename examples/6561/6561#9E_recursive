#This is a new presentation of obrien9 given in 
#"On the Efficiency of some finite groups"
_RWS := rec
(
  isRWS := true,
  ordering := "recursive",
  generatorOrder := [b,B,a,A,c,C],
  inverses := [B,b,A,a,C,c],
  equations :=
  [
      [a*a*B*a*b,IdWord],
      [b*b*C*a*c*b*A,IdWord],
      [c*c*B*c*A*b*a,IdWord],
#      [C*b^2*c*c*B*C*B,IdWord]
  ]
);

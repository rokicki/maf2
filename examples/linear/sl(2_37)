#presentation of sl(2,p) for p=37
_RWS := rec
(
  isRWS := true,
  generatorOrder := [x,X,y,Y],
  ordering := "shortlex",
  inverses := [X,x,Y,y],
  equations :=
  [
    [x*x*(Y*X)^3,IdWord],
    [(x*y^4*x*y^19)^2*y^37*x^24,IdWord],
    [x^4,IdWord],
    [x^2*y^37,IdWord],
    [x^2*y,y*x^2]
  ]
);

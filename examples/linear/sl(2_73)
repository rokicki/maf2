# presentation of sl(2,p) for p=73
_RWS := rec
(
  isRWS := true,
  generatorOrder := [x,X,y,Y],
  ordering := "shortlex",
  inverses := [X,x,Y,y],
  equations :=
  [
    [x*x*(Y*X)^3,IdWord],
    [(x*y^4*x*y^37)^2*y^73*x^48,IdWord],
    [x^4,IdWord],
    [x^2*y^73,IdWord],
    [x^2*y,y*x^2]
  ]
);

#Sieradski group g8(2,1)
_RWS := rec
(
  isRWS := true,
  ordering := "shortlex",
  generatorOrder := [a,A,b,B,c,C,d,D,e,E,f,F,g,G,h,H],
        inverses := [A,a,B,b,C,c,D,d,E,e,F,f,G,g,H,h],
  equations :=
  [
    [a*c,b],
    [b*d,c],
    [c*e,d],
    [d*f,e],
    [e*g,f],
    [f*h,g],
    [g*a,h],
    [h*b,a]
  ]
);

_RWS := rec
(
  isRWS := true,
  ordering := "shortlex",
  generatorOrder := [s1,s1^-1,s2,s2^-1,s3,s3^-1,s4,s4^-1],
        inverses := [s1^-1,s1,s2^-1,s2,s3^-1,s3,s4^-1,s4],
  equations :=
  [
    [s2*s1*s2,s1*s2*s1],
    [s3*s1,s1*s3],
    [s4*s1,s1*s4],
    [s3*s2*s3,s2*s3*s2],
    [s4*s2,s2*s4],
    [s4*s3*s4,s3*s4*s3]
  ]
);

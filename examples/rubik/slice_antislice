    [tt*rl*fb*LR*dd*fb*rl*BF,IdWord],
    [rr*tt*rr*tt*rr*rl*ff*rr*ff*LR,IdWord],
    [rr*ff*rr*ff*rr*rl*dd*rr*dd*LR,IdWord],
    [rr*tt*ll*ff*fb*dd*ll*tt*ff*BF,IdWord],
    [tt*ll*dd*ff*fb*rr*dd*ll*ff*BF,IdWord],
    [BF*ll*tt*BF*rl*ff*tt*rl,fb*rr*tt*fb*LR*ff*tt*LR],
    [LR*BF*tt*LR*BF*tt*LR*BF*tt,rl*fb*dd*rl*fb*dd*rl*fb*dd],
    [tt*rr*ff*bb*ll,rr*ff*bb*ll*dd],
    [tt*ff*tt*rr*ll,rr*ll*dd*bb*dd],
    [dd*rr*tt*rr*ll,rr*ll*tt*rr*dd],
    [dd*rr*ff*rr*tt,tt*ll*bb*ll*dd],
    [ff*rr*ff*rr*ll,rr*ll*bb*rr*bb],
    [ff*bb*tt*ff*tt,dd*ff*dd*ff*bb],
    [bb*dd*rr*dd*ff,ff*tt*ll*tt*bb],
    [td*rl*td*rl*td*rl,LR*DT*LR*DT*LR*DT],
    [td*fb*rl*td*fb*rl,rl*fb*td*rl*fb*td],
    [td*fb*rl*DT*fb*rl,rl*fb*DT*rl*fb*td],
    [ll*ff*rr*tt*ff*rr,rr*bb*rr*dd*bb*ll],
    [tt*rr*tt*rr*tt*rr,rr*dd*ll*tt*ll*dd],
    [dd*bb*ll*tt*ff*tt,tt*ff*rr*tt*bb*dd],

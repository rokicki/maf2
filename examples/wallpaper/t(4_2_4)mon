#P4MM triangle group presentation
#    b  +                          
#     / |a                         
#   +---+       
#     c                               
_RWS := rec
(
  isRWS := true,
  generatorOrder := [a,b,c],
  ordering := "shortlex",
  inverses := [,,],
  equations := 
  [
    [a*a,IdWord],
    [b*b,IdWord],
    [c*c,IdWord],
    [c*a*c*a,IdWord],
    [b*a*b*a*b*a*b*a,IdWord],
    [c*b*c*b*c*b*c*b,IdWord]
  ]
);

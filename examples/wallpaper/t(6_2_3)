#P6MM triangle group presentation
#    b  +                   
#     / |a                       
#   +---+       
#     c                                
_RWS := rec
(
  isRWS := true,
  generatorOrder := [a,b,c],
  ordering := "shortlex",
  inverses := [a,b,c],
  equations := 
  [
    [(b*c)^6,IdWord],
    [(c*a)^2,IdWord],
    [(a*b)^3,IdWord]
  ]
);

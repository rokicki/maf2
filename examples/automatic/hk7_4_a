#Hyperbolic Knot 7 4 with extra generator a
#see hk4_1_a for explanation
_RWS := rec
(
  isRWS := true,
  ordering := "shortlex",
  generatorOrder := [a,A,x,X,y,Y,z,Z,t,T,u,U,v,V,w,W],
        inverses := [A,a,X,x,Y,y,Z,z,T,t,U,u,V,v,W,w],
  equations :=
  [
    [A*v*U*X*W*Y*Z*T,IdWord],
    [T*x*z*X,IdWord],
    [x*v*Y*V,IdWord],
    [y*u*Y*v,IdWord],
    [u*y*U*Z,IdWord],
    [w*t*W*U,IdWord],
    [t*w*T*X,IdWord],
  ]
);

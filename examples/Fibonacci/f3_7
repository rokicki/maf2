#Fibonacci group F(3,7).
_RWS := rec
(
  isRWS := true,
  ordering := "shortlex",
  generatorOrder := [a,A,b,B,c,C,d,D,e,E,f,F,g,G],
        inverses := [A,a,B,b,C,c,D,d,E,e,F,f,G,g],
  equations :=
  [
    [a*b*c,d],
    [b*c*d,e],
    [c*d*e,f],
    [d*e*f,g],
    [e*f*g,a],
    [f*g*a,b],
    [g*a*b,c]
  ]
);
